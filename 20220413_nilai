#include <iostream>
using namespace std;

int main(){
    
    int nilai;
    string keterangan;
    cout<<"Masukkan Nilai = ";cin>>nilai;
    
    if (nilai >100 || nilai <0){
        keterangan = "diluar_jangkauan";
    }
    else if (nilai >= 90){
        keterangan = "Sangat_Memuaskan";
    }
    else if (nilai >= 80){
        keterangan = "Memuaskan";
    }
    else if (nilai >= 75){
        keterangan = "cukup";
    }
    else {
        keterangan = "Tidak_lulus";
    }
}
