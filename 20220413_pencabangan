#include <iostream>
using namespace std;
int main()
{
    int nilai;
    if (nilai % 2 == 0) {
        cout<<"bilangan genap";
        if (nilai <10) {
            cout<<"Bilangan genap kurang dari 10"<<endl;
        }
        else {
            cout<<"bilangan genap lebih dari 10"<<endl;
        }
    }
    else {
        cout<<"Bilangan Ganjil";
        if (nilai <10) {
            cout<<"bilangan ganjil kurang dari 10"<<endl;
        }
        else {
            cout<<"bilangan ganjil lebih dari 10"<<endl;
        }
    }
}
